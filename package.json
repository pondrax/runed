{
  "name": "runed",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite-node build/index.js",
    "serve": "pm2 delete my-app || true && pm2 start pnpm --name my-app -- run start",
    "stop": "pm2 delete my-app",
    "logs": "pm2 logs my-app",
    "status": "pm2 monit",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.22.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/vite": "^4.1.11",
    "daisyui": "^5.0.43",
    "dayjs": "^1.11.13",
    "drizzle-kit": "1.0.0-beta.1-c0277c0",
    "iconify-icon": "^3.0.0",
    "runed": "^0.29.1",
    "svelte": "^5.34.8",
    "svelte-check": "^4.2.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.1.0",
    "drizzle-orm": "1.0.0-beta.1-c0277c0",
    "postgres": "^3.4.7",
    "vite-node": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}